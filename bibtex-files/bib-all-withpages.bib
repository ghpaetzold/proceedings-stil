@INPROCEEDINGS{171726,
    AUTHOR = "Rodrigues, Júlia and Pagano, Adriana and Paraiso, Emerson",
    TITLE = "Estudo exploratório de categorias gramaticais com potencial de indicadores na Análise de Sentimentos",
    BOOKTITLE = "Proceedings of the 11th Brazilian Symposium in Information and Human Language Technology",
    ADDRESS = "Uberlândia, Brazil",
    DAYS = "2-5",
    MONTH = "October",
    YEAR = "2017",
    ABSTRACT = "Este trabalho apresenta uma pesquisa em andamento sobre categorias gramaticais que podem ser exploradas como indicadores de sentimentos no texto. Diferentemente de pesquisas sobre análise de sentimentos que se concentram em itens lexicais, este estudo baseia-se na gramática sistêmico, a fim de mapear padrões na escolha de sistemas gramaticais que podem ser associados à construção de sentimentos na linguagem. A metodologia baseia-se na anotação manual de amostras de textos do tipo notícia, contendo os principais sistemas gramaticais no nível da oração e do grupo na escala de ordens. A frequência das categorias anotadas e dos agrupamentos das mesmas será investigada para verificar se aponta para as categorias mais produtivas na análise de sentimentos em textos.",
    KEYWORDS = "Sentiment Analysis and Opinion Mining",
    URL = "http://www.aclweb.org/anthology/W17-6602",
    pages = "17-21",
    organization = "Sociedade Brasileira de Computação",
    editor = "Gustavo Henrique Paetzold and Vládia Pinheiro"
}

